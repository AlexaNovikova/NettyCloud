<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="241.0" prefWidth="499.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AuthDialogController">
   <children>
      <Label layoutX="51.0" layoutY="60.0" prefHeight="37.0" prefWidth="81.0" text="Логин:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="51.0" layoutY="126.0" text="Пароль:">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="loginField" layoutX="139.0" layoutY="53.0" prefHeight="39.0" prefWidth="316.0">
         <font>
            <Font size="18.0" />
         </font>
         <tooltip>
            <Tooltip text="Enter Login" />
         </tooltip>
      </TextField>
      <PasswordField fx:id="passField" layoutX="139.0" layoutY="114.0" onAction="#checkAuth" prefHeight="39.0" prefWidth="316.0">
         <font>
            <Font size="18.0" />
         </font>
         <tooltip>
            <Tooltip text="Enter password" />
         </tooltip>
      </PasswordField>
      <Button layoutX="139.0" layoutY="181.0" mnemonicParsing="false" onAction="#checkAuth" prefHeight="39.0" prefWidth="93.0" text="Вход">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="309.0" layoutY="181.0" mnemonicParsing="false" onAction="#registerOpen" prefHeight="39.0" prefWidth="145.0" text="Регистрация">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
